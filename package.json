{
  "name": "learning-rxjs",
  "version": "0.0.1",
  "description": "learning-rxjs",
  "author": "luotao",
  "scripts": {
    "dev": "rimraf ./dist && webpack && node ./dist/index.js",
    "dev:node": "rimraf ./dist && webpack --target node && node ./dist/index.js",
    "dev:node:debug": "rimraf ./dist && webpack --target node && node %NODE_OPTIONS% ./dist/index.js",
    "build": "rimraf ./dist && webpack --mode production",
    "build:node": "rimraf ./dist && webpack --target node --mode production"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.29",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.4.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
